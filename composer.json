{
  "name": "drupal/event",
  "type": "drupal-module",
  "description": "Drupal module for creating events",
  "keywords": ["Drupal, COD", "Events"],
  "homepage": "https://github.com/drupal-cod/event",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "event": {
      "type": "vcs",
      "url": "https://github.com/drupal-cod/event.git"
    }
  },
  "require-dev": {
    "drupal/diff": "*@dev",
    "drupal/field_permissions": "^1.0@beta",
    "drupal/entity_embed": "1.0.0-beta2",
    "drupal/entity": "^1.0.0-beta1",
    "drupal/entity_browser": "2.0-alpha2"
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
    }
  }
}
